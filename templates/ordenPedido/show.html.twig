
{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Orden Pedidos' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'ordenPedido_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
        {{ action.tbl_tr('Trabajador',ordenPedido.trabajador) }}
        {{ action.tbl_tr('Almacen',ordenPedido.almacen) }}
        {{ action.tbl_tr('Cliente',ordenPedido.cliente) }}
        {{ action.tbl_tr('FechaPedido',ordenPedido.FechaPedido) }}
        {{ action.tbl_tr('Despachar',ordenPedido.despacho)? 'Yes' : 'No' }}
        {% include 'crud/_show_info.html.twig' with {'entity': ordenPedido} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'ordenPedido','entity': ordenPedido} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'ordenPedido','entity': ordenPedido} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'ordenPedido','entity': ordenPedido} %}
{% endblock %}
