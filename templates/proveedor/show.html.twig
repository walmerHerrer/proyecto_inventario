{% extends 'base.html.twig' %}
{% import 'macro/breadcrumb.html.twig' as breadcrumb %}
{% import 'macro/action.html.twig' as action %}

{% set _title = 'Proveedor' %}
{% set _main_title = 'Mostrar' %}
{% set _section =  'proveedor_index'%}

{% block _breadcrumb %}
    {{ breadcrumb.show2(_title, _section, 'Mostrar') }}
{% endblock %}

{% block _main %}
    {{ action.tbl_start() }}
        {{ action.tbl_tr('Nombre',proveedor.nombre) }}
        {{ action.tbl_tr('Apellidos',proveedor.apellidos) }}
        {{ action.tbl_tr('Direccion',proveedor.direccion) }}
        {{ action.tbl_tr('Telefono',proveedor.telefono) }}
        {{ action.tbl_tr('Dni',proveedor.dni) }}
        {% include 'crud/_show_info.html.twig' with {'entity': proveedor} %}
    {{ action.tbl_end() }}

{% endblock %}

{% block _main_footer %}
    {% include 'crud/_show_links.html.twig' with {'route_base':'proveedor','entity': proveedor} %}
{% endblock %}

{% block _main_tools %}
    {% include 'crud/_tools_ico.html.twig' %}
{% endblock %}

{% block _body_tools %}
    {% include 'crud/_show_links_ico.html.twig' with {'route_base':'proveedor','entity': proveedor} %}
{% endblock %}

{% block _body_tools_ico %}
    {% include 'crud/_show_links_tool.html.twig' with {'route_base':'proveedor','entity': proveedor} %}
{% endblock %}